{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "images",
      "cacheConfig": {
        "maxAge": "1d",
        "maxSize": 50,
        "strategy": "freshness",
        "timeout": "10s"      
      },
      "urls": [
        "**firebasestorage.googleapis.com/**"
      ]
    },
    {
      "name": "Firestore API calls",
      "cacheConfig": {
        "maxAge": "30m",
        "maxSize": 20,
        "strategy": "freshness",
        "timeout": "10s"
      },
      "urls": [
        "**firestore.googleapis.com/**",
        "**googleapis.com/identitytoolkit**"
      ]
    }
  ]
}
